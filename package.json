{
  "name": "workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "cross-env ENV_DEV=development npx nodemon",
    "dev:server-client": "concurrently \"npm run server\" \"npm run client\"",
    "dev:server": "npm run server",
    "dev:client": "npm run client",
    "server": "nodemon ./build/main",
    "client": "npm run dev --prefix client",
    "client:build": "npm run build --prefix client",
    "build": "rimraf ./build && npx tsc && npm run client:build",
    "start": "npm run build && npm run start:prod",
    "start:prod": "cross-env ENV_PROD=production node build/src/main",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix","server-dependencies": "npm install",
    "client-dependencies": "npm install --prefix client",
    "install-all-deps": "npm install && concurrently \"npm run server-dependencies\" \"npm run client-dependencies\""
    
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.11",
    "@types/nodemailer": "^6.4.16",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.19.1",
    "rimraf": "^5.0.6",
    "tsx": "^4.9.4",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "ip": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.11.5",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "uuid": "^10.0.0"
  }
}
